<div class="container">
  <div class="row mt-5">
    <%= @live.name %>のライブハウスは？
  </div>
  <div class="row mt-3">
    <%= form_with model: LiveSchedule.new, url: admin_live_live_schedules_path, method: :post do |f| %>
      <%= f.collection_select :live_house_id, LiveHouse.all, :id, :name, include_blank: "選択して下さい" %>
      <%= f.hidden_field :live_id, value: @live.id %>
      <%= f.submit "登録" %>
    <% end %>
  </div>
</div>